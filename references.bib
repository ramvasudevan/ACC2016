	Shorttitle = {Kinematic creep in a continuously variable transmission},
	Title = {Kinematic creep in a continuously variable transmission: traction drive mechanics for cobots},
	Url = {http://dx.doi.org/10.1115/1.1517560},
	Volume = {124},
	Year = {2002},
