	Month = sep,
	Number = {37},
	Pages = {11523--11539},
	Shorttitle = {The representation of tool use in humans and monkeys},
	Title = {The representation of tool use in humans and monkeys: common and uniquely human features},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/19759300},
