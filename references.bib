	Pages = {301--308},
	Shorttitle = {Learning in a virtual environment using haptic systems for movement re-education},
	Title = {Learning in a virtual environment using haptic systems for movement re-education: can this medium be used for remodeling other behaviors and actions?},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/21527097},
	Volume = {5},
