	Keywords = {Amputees,Amputees: rehabilitation,Arm,Artificial Limbs,Artificial Limbs: psychology,Artificial Limbs: utilization,Humans,Patient Satisfaction,Prosthesis Design,Quality of Life,Treatment Refusal,Treatment Refusal: statistics \& numerical data,Upper Extremity},
	Month = sep,
	Number = {3},
	Pages = {236--57},
	Pmid = {17979010},
	Title = {{Upper limb prosthesis use and abandonment: a survey of the last 25 years.}},
