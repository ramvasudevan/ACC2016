	Keywords = {Equipment Design, Female, Gait, Humans, Male, Middle Aged, Paresis, Recovery of Function, rehabilitation},
	Month = sep,
	Number = {9},
	Pages = {1158--1161},
	Title = {A mechanized gait trainer for restoring gait in nonambulatory subjects},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/10987154},
	Volume = {81},
	Year = {2000},
